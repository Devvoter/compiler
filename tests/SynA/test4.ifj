// funkci s promennou, ktera se vypocita jako vyraz. napriklad (a+ b - 3 /6)  

const ifj = @import("ifj24.zig");


pub fn main() void {
    var a: f64 = 2.0;
    var b: f64 = 4.0;
    var c = a + b - b/a; // 4

    var d = 1 + 2 + (a-b)*a; // -1

    ifj.write("c = "); ifj.write(c); ifj.write("\n");
    ifj.write("d = "); ifj.write(d); ifj.write("\n");
}